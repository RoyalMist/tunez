SECRETS=$(kamal secrets fetch --adapter 1password --account bazzile.1password.com --from Tech/Tunez KAMAL_REGISTRY_PASSWORD DATABASE_URL SECRET_KEY_BASE TOKEN_SIGNING_SECRET)
KAMAL_REGISTRY_PASSWORD=$(kamal secrets extract KAMAL_REGISTRY_PASSWORD ${SECRETS})y)
DATABASE_URL=$(kamal secrets extract DATABASE_URL ${SECRETS})
SECRET_KEY_BASE=$(kamal secrets extract SECRET_KEY_BASE ${SECRETS})
TOKEN_SIGNING_SECRET=$(kamal secrets extract TOKEN_SIGNING_SECRET ${SECRETS})
